global: 
  scrape_interval: 10s 
scrape_configs: 
  - job_name: 'prometheus_master' 
    scrape_interval: 5s 
    static_configs: 
      - targets: ['{{ prometheus_web_listen_address }}'] 
  - job_name: 'node_exporter_debian'
    scrape_interval: 5s
    static_configs:
      - targets: ['{{ prometheus_node_exporter_listen_address }}']
